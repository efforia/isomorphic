{
  "name": "fretefacil-app",
  "version": "0.9.0",
  "homepage": "./",
  "private": false,
  "scripts": {
    "start": "node app.js",
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "resources": "capacitor-resources",
    "deploy": "node scripts/deploy/cloud",
    "run:android": "npm run build && capbox run android",
    "build:android": "npm run build && capbox build android --release",
    "distribute:android:dev": "npm run build && capbox distribute android --stage=development",
    "stats": "react-scripts build --stats && webpack-bundle-analyzer ./build/bundle-stats-json"
  },
  "dependencies": {
    "@capacitor/android": "1.0.0-beta.22",
    "@capacitor/cli": "1.0.0-beta.22",
    "@capacitor/core": "1.0.0-beta.22",
    "@fnando/cpf": "^0.1.1",
    "axios": "^0.18.0",
    "cordova": "9.0.0",
    "cordova-plugin-device": "2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "framework7": "^4.2.0",
    "framework7-react": "^4.2.0",
    "hellojs": "^1.18.1",
    "moment-mini": "^2.22.1",
    "react": "^16.7.0",
    "react-app-polyfill": "^1.0.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-lottie": "^1.2.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-filter": "^0.0.18",
    "redux-thunk": "^2.3.0",
    "validator": "^10.11.0",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "capacitor-resources": "^1.1.2",
    "capbox": "^1.2.3",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.12.3",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "inquirer": "^6.3.1",
    "ip": "^1.1.5",
    "klaw-sync": "^6.0.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.8.0",
    "prop-types": "15.x",
    "react-scripts": "2.1.8",
    "shelljs": "^0.8.3",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
