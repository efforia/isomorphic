{
  "appId": "com.efforia.fretefacil",
  "appName": "Owen",
  "jsFramework": "react",
  "npmClient": "yarn",
  "webDir": "build",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": true,
      "androidSplashResourceName": "splash",
      "androidScaleType": "CROP_CENTER"
    }
  },
  "distribute": {
    "appcenter": {
      "organization": "Efforia",
      "ios": {
        "development": "Efforia/Owen-Development",
        "homolog": "Efforia/Owen-Homolog",
        "production": "Efforia/Owen"
      },
      "android": {
        "development": "Efforia/Owen-Android-Development",
        "homolog": "Efforia/Owen-Android-Homolog",
        "production": "Efforia/Owen-Android"
      }
    }
  }
}